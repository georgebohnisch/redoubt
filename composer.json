{
    "name": "greggilbert/redoubt",
    "description": "A resource-level ACL for Laravel 4.",
	"keywords": ["acl", "resource", "laravel", "laravel4"],
	"homepage": "http://github.com/greggilbert/redoubt",
	"license": "MIT",
    "authors": [
        {
            "name": "Greg Gilbert",
            "email": "greg@greg-gilbert.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Greggilbert\\Redoubt": "src/"
        }
    },
	"extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "minimum-stability": "dev"
}